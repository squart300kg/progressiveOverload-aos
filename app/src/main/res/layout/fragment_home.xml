<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:theWarsOfStar="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
<!--        <variable-->
<!--            name="homeVm"-->
<!--            type="com.example.war.of.thewarofstars.ui.home.HomeViewModel" />-->
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        theWarsOfStar:context=".ui.home.HomeFragment">

        <include
            android:id="@+id/layout_add_program"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:layout_margin="20dp"
            app:layout_constraintTop_toTopOf="parent"
            layout="@layout/layout_adding_program" />

        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="10dp"
            theWarsOfStar:listitem="@layout/item_program"
            app:layout_constraintTop_toBottomOf="@id/layout_add_program"
            app:layout_constraintBottom_toBottomOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
